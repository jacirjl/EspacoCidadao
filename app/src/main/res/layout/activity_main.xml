<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_google_signin_btn_text_dark_default"
    tools:context="com.example.jacirjlseae.espacocidadao.MainActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="240dp"
        android:layout_height="130dp"
        android:layout_margin="10dp"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="8dp"
        android:foreground="@drawable/logo"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toTopOf="@+id/Ativos"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.537" />

    <Button
        android:id="@+id/Ativos"
        android:layout_width="193dp"
        android:layout_height="69dp"
        android:layout_marginBottom="30dp"
        android:text="Espaços Cidadãos Ativos"
        app:layout_constraintBottom_toTopOf="@+id/SemECs"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="272dp" />

    <Button
        android:id="@+id/SemECs"
        android:layout_width="196dp"
        android:layout_height="71dp"
        android:layout_marginBottom="100dp"
        android:text="Municipios Sem Espaços Cidadãos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/versao"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>
